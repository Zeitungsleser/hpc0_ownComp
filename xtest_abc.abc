@ <stdio.hdr>
@ "parser.hdr"
@ "lexer.hdr"
@ "genvar.hdr"

fn main()
{
    getToken();
    while(true) {
        local expr: -> Expr = parseStatement();
        if (!expr) {
            break;
        }
        genReleaseReg(loadExpr(expr));
    }
    if (token.kind != EOI) {
        printf("%u.%u: syntax error\n", token.pos.row, token.pos.col);
    }
    genHalt(3);
    genPrintSymtab();
}
